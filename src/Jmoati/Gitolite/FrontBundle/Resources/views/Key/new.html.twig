{% extends 'JmoatiGitoliteFrontBundle::base.html.twig' %}


{% form_theme form 'JmoatiTwitterBootstrapBundle::form_theme.html.twig' %}

{% block container_content %}
    <form action="{{ path('jmoati_gitolite_front_key_new') }}" method="post" {{ form_enctype(form) }}>
        <legend>{% trans %}key.create.title{% endtrans %}</legend>

        <div class="well">
            {{ form_rest(form) }}
        </div>

        <a href="{{ path('jmoati_gitolite_front_key_index') }}">{% trans %}action.cancel{% endtrans %}</a>
        <button type="submit" class="btn btn-success">{% trans %}action.create{% endtrans %}</button>
    </form>
{% endblock %}
